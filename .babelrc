{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/transform-runtime",
        [
            "wildcard",
            {
                "exts": [
                    "js",
                    "es6",
                    "es",
                    "jsx",
                    "png"
                ]
            }
        ]
    ]
}
